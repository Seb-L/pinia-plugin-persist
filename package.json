{
  "name": "pinia-plugin-persist",
  "version": "0.0.0",
  "description": "Persist pinia state data in sessionStorage or other storages.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "module": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build:esm": "esbuild src/index.ts --bundle --outfile=dist/index.esm.js --external:vue --format=esm",
    "build:cjs": "esbuild src/index.ts --bundle --outfile=dist/index.cjs.js --external:vue --format=cjs",
    "build": "rimraf dist && yarn build:esm && yarn build:cjs",
    "lint": "eslint . --ext .ts"
  },
  "repository": "https://seb-l@github.com/Seb-L/pinia-plugin-persist.git",
  "author": {
    "name": "Sebastien Lombard",
    "email": "sebastien.lombard75@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vuex",
    "store",
    "pinia",
    "pinia-plugin",
    "piÃ±a",
    "pigna",
    "composition",
    "api",
    "setup",
    "typed",
    "typescript",
    "ts",
    "type",
    "safe"
  ],
  "dependencies": {
    "vue-demi": "0.12.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "2.2.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "@vue/composition-api": "1.4.1",
    "@vue/devtools-api": "6.0.0-beta.20.1",
    "@vue/test-utils": "1.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "esbuild": "0.14.3",
    "eslint": "8.4.1",
    "jest": "27.4.4",
    "jest-mock-warn": "^1.1.0",
    "lint-staged": "12.1.2",
    "pinia": "2.0.6",
    "rimraf": "^3.0.2",
    "typescript": "4.5.3",
    "vuepress": "^1.8.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  }
}
